class Solution {
    public int singleNonDuplicate(int[] nums) {
        int x =0;
        for(int i=0;i<nums.length;i++){
            int count=0;
            x = nums[i];
            for(int j=0;j<nums.length;j++){
                if(nums[j]==x && i!=j)
                    count =1;   
                 // continue;
                }
                 
        
         if(count == 1)
             continue;
        else 
            return nums[i];
        }
        return 0;
    }
}
